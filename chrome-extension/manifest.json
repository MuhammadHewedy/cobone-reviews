{
    "name": "__MSG_appName__",
    "description": "__MSG_appDesc__",
    "default_locale": "ar",
    "version": "0.2",
    "permissions": [
        "cookies",
        "<all_urls>"
    ],
    "content_scripts": [{
        "matches": ["https://www.cobone.com/*"],
        "js": ["lib/jquery.min.js", "messages.js", "translate.js", "comments.js"],
        "run_at": "document_end"
    }],
    "manifest_version": 2,
    "web_accessible_resources": ["comments.html", "lib/recaptcha_api_ar.js", "lib/recaptcha_api_en.js"],
    "icons": {
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    }
}
